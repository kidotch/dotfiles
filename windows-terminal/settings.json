  "actions": [
    {
      "command": {
        "action": "sendInput",
        "input": "Set-TerminalDark\r"
      },
      "id": "User.setDark"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "Set-TerminalLight\r"
      },
      "id": "User.setLight"
    },
    {
      "command": {
        "action": "adjustOpacity",
        "opacity": 5,
        "relative": true
      },
      "id": "User.opacityUp"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "tmux attach -t work\n"
      },
      "id": "User.sendInput.9CBCC12D",
      "name": "tmux attach"
    },
    {
      "command": {
        "action": "adjustOpacity",
        "opacity": -5,
        "relative": true
      },
      "id": "User.opacityDown"
    },
    {
      "command": {
        "action": "splitPane",
        "type": "snippets"
      },
      "id": "User.splitPane.A2B727CC"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "rclone config create gdrive drive token='JSON_TOKEN'"
      },
      "id": "User.sendInput.RCLONE04",
      "name": "rclone config create gdrive (token)"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "tmux new -s work\n"
      },
      "id": "User.sendInput.7F71203C",
      "name": "tmux new"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "rclone copy datasets gdrive:datasets -P\n"
      },
      "id": "User.sendInput.5135683A",
      "name": "rclone upload"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "export HOME=/dev/shm/home && mkdir -p $HOME"
      },
      "id": "User.sendInput.HOME0001",
      "name": "export HOME /dev/shm"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "git add . && git commit -m 'update' && git push\n"
      },
      "id": "User.sendInput.6F1BD755",
      "name": "git upload"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "git clone https://github.com/26hzhang/VSLNet.git\n"
      },
      "id": "User.sendInput.503D3E25",
      "name": "git clone VSLNet"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "export RCLONE_CONFIG=/dev/shm/rclone.conf"
      },
      "id": "User.sendInput.RCLONE02",
      "name": "export RCLONE_CONFIG /dev/shm"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "curl -fsSL https://claude.ai/install.sh | bash"
      },
      "id": "User.sendInput.CLAUDE001",
      "name": "claude install"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "export RCLONE_CONFIG_PASS=''"
      },
      "id": "User.sendInput.RCLONE03",
      "name": "export RCLONE_CONFIG_PASS empty"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "export RCLONE_CONFIG=/tmp/rclone.conf"
      },
      "id": "User.sendInput.RCLONE01",
      "name": "export RCLONE_CONFIG /tmp"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "curl -fsSL https://deb.nodesource.com/setup_22.x | bash - && apt-get install -y nodejs"
      },
      "id": "User.sendInput.NODE2201",
      "name": "install nodejs 22"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "echo 'export PATH=\\\"$HOME/.local/bin:$PATH\\\"' >> ~/.bashrc && source ~/.bashrc"
      },
      "id": "User.sendInput.PATH0001",
      "name": "add ~/.local/bin to PATH"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "$token = Get-Secret rclone-gdrive -AsPlainText"
      },
      "id": "User.sendInput.PS000001",
      "name": "pwsh get rclone token"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "printf '%s\n' \"export PS1='\\[\\e[1;32m\\]\\u@\\h:\\[\\e[1;36m\\]\\w\\[\\e[0m\\]\\$ '\" >> ~/.bashrc\nsource ~/.bashrc\n"
      },
      "id": "User.sendInput.6A996637",
      "name": "set PS1 (bash) + persist"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "vastai create instance <OFFER_ID> --image vastai/base-image:@vastai-automatic-tag --env '-p 1111:1111 -p 6006:6006 -p 8080:8080 -p 8384:8384 -p 72299:72299 -e OPEN_BUTTON_PORT=1111 -e OPEN_BUTTON_TOKEN=1 -e JUPYTER_DIR=/ -e DATA_DIRECTORY=/workspace/ -e PORTAL_CONFIG=\"localhost:1111:11111:/:Instance Portal|localhost:8080:18080:/:Jupyter|localhost:8080:8080:/terminals/1:Jupyter Terminal|localhost:8384:18384:/:Syncthing|localhost:6006:16006:/:Tensorboard\"' --onstart-cmd 'entrypoint.sh' --disk 32 --jupyter --ssh --direct"
      },
      "id": "User.sendInput.1CF0D5CE",
      "name": "vastai templatecommand"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "vastai create instance <id> --image vastai/base-image:@vastai-automatic-tag --disk 256 --ssh --direct --env \"-e RCLONE_CONFIG_B64=$([Convert]::ToBase64String([IO.File]::ReadAllBytes('C:\\Users\\ahcha\\AppData\\Roaming\\rclone\\rclone.conf')))\" --onstart-cmd 'bash -c \"curl -fsSL https://raw.githubusercontent.com/kidotch/dotfiles/main/bootstrap.sh | bash\"'"
      },
      "id": "User.sendInput.VAI256001",
      "name": "vastai create 256GB + rclone + dotfiles"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "curl -sL https://raw.githubusercontent.com/kidotch/dotfiles/main/bootstrap.sh | bash"
      },
      "id": "User.sendInput.DOTFILE01",
      "name": "dotfiles bootstrap"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "(type -p wget >/dev/null || (sudo apt update && sudo apt install wget -y)) && sudo mkdir -p -m 755 /etc/apt/keyrings && out=$(mktemp) && wget -nv -O$out https://cli.github.com/packages/githubcli-archive-keyring.gpg && cat $out | sudo tee /etc/apt/keyrings/githubcli-archive-keyring.gpg > /dev/null && sudo chmod go+r /etc/apt/keyrings/githubcli-archive-keyring.gpg && sudo mkdir -p -m 755 /etc/apt/sources.list.d && echo \"deb [arch=$(dpkg --print-architecture) signed-by=/etc/apt/keyrings/githubcli-archive-keyring.gpg] https://cli.github.com/packages stable main\" | sudo tee /etc/apt/sources.list.d/github-cli.list > /dev/null && sudo apt update && sudo apt install gh -y"
      },
      "id": "User.sendInput.9E5B525",
      "name": "install gh"
    }
  ],
